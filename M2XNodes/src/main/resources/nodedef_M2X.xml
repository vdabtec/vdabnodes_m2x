<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<nodedefs>
<nodedef name="M2XService" class="vdab.extnodes.m2x.M2XService">
	<attr name="Group">Data</attr>
	<attr name="ConsumedEventTypes">Event</attr>
    <attr name="PublishedEventTypes">Event</attr>
    <attr name="NodeOverview">Writes data to an ATT M2X Server</attr>   
    <attr name="NodeDescTemplate"> Push to M2X </attr>
	<attr name="NodeQuality" >Beta</attr>  
	<datadef name="DeviceID">
		<attr name="EditOrder" type="Integer">21</attr>
		<attr name="Category">Required</attr>
		<attr name="Desc">The device ID assigned to this M2X device.</attr>		
	</datadef>
	<datadef name="ApiKey">
		<attr name="EditOrder" type="Integer">22</attr>
		<attr name="Category">Required</attr>
		<attr name="Desc">The API Key assigned to this M2X device.</attr>		
	</datadef>
	<datadef name="UseHTTPS">
		<attr name="EditOrder" type="Integer">23</attr>
		<attr name="Category">Required</attr>
		<attr name="Desc">Set to TRUE to POST data using HTTPS.</attr>		
	</datadef>
	 <datadef name="SelectedElement">
        <attr name="Category">Standard</attr>
        <attr name="EditWhileRunning" type="Boolean">true</attr>
        <attr name="EditOrder" type="Integer">24</attr>
        <attr name="Desc">Select the data to be sent to M2X.</attr>
    </datadef>
	<datadef name="Stream">
        <attr name="Category">Standard</attr>
        <attr name="EditWhileRunning" type="Boolean">true</attr>
        <attr name="EditOrder" type="Integer">25</attr>
        <attr name="Desc">Name of stream to push data to.</attr>
    </datadef>
	<datadef name="OutputType">
        <attr name="Category">Hidden</attr>
    </datadef>
</nodedef>
</nodedefs>